@model List<ASP.Net_MVC.Models.Employee>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h1 style="text-align:center;">CRUD Operations using ADO.Net with ASP.Net MVC</h1>
        <hr />
        <a href="/Home/RegisterEmp">Register New Employee</a>
        <br />
        <table border="1" align="center">
            <tr>
                <th>Eno</th>
                <th>Ename</th>
                <th>Job</th>
                <th>Salary</th>
                <th>Dname</th>
                <th style="text-align:center;">Edit/Delete</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Eno</td>
                    <td>@item.Ename</td>
                    <td>@item.Job</td>
                    <td>@item.Salary</td>
                    <td>@item.Dname</td>
                    <td>
                        <a href="/Home/Edit/@item.Eno">Edit</a>
                        <a href="/Home/Delete/@item.Eno">Delete</a>
                    </td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
